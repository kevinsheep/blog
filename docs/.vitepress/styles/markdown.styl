/**
 * Paragraph and inline elements
     * -------------------------------------------------------------------------- */
.md-preview p, .md-preview summary {
  margin: 16px 0;
}

.md-preview p {
  line-height: 28px;
}

.md-preview blockquote {
  margin: 16px 0;
  border-left: 2px solid var(--vp-c-divider);
  padding-left: 16px;
  transition: border-color 0.5s;
}

.md-preview blockquote > p {
  margin: 0;
  font-size: 16px;
  color: var(--vp-c-text-2);
  transition: color 0.5s;
}

.md-preview a {
  font-weight: 500;
  color: var(--vp-c-brand);
  text-decoration-style: dotted;
  transition: color 0.25s;
}

.md-preview a:hover {
  color: var(--vp-c-brand-dark);
}

.md-preview strong {
  font-weight: 600;
}

/**
 * Lists
     * -------------------------------------------------------------------------- */
.md-preview ul, .md-preview ol {
  padding-left: 1.25rem;
  margin: 16px 0;
}

.md-preview ul {
  list-style: disc;
}

.md-preview ol {
  list-style: decimal;
}

.md-preview li + li {
  margin-top: 8px;
}

.md-preview li > ol, .md-preview li > ul {
  margin: 8px 0 0;
}

/**
 * Table
     * -------------------------------------------------------------------------- */
.md-preview table {
  display: block;
  border-collapse: collapse;
  margin: 20px 0;
  overflow-x: auto;
}

.md-preview tr {
  border-top: 1px solid var(--vp-c-divider);
  transition: background-color 0.5s;
}

.md-preview tr:nth-child(2n) {
  background-color: var(--vp-c-bg-soft);
}

.md-preview th, .md-preview td {
  border: 1px solid var(--vp-c-divider);
  padding: 12px 16px;
}

.md-preview th {
  font-size: 16px;
  font-weight: 600;
  background-color: var(--vp-c-white-soft);
}

.dark .md-preview th {
  background-color: var(--vp-c-black);
}

/**
 * Decorational elements
     * -------------------------------------------------------------------------- */
.md-preview hr {
  margin: 16px 0;
  border: none;
  border-top: 1px solid var(--vp-c-divider-light);
}

/**
 * Custom Block
     * -------------------------------------------------------------------------- */
.md-preview .custom-block {
  margin: 16px 0;
}

.md-preview .custom-block p {
  margin: 8px 0;
  line-height: 24px;
}

.md-preview .custom-block p:first-child {
  margin: 0;
}

.md-preview .custom-block a {
  color: inherit;
  font-weight: 600;
  text-decoration: underline;
  transition: opacity 0.25s;
}

.md-preview .custom-block a:hover {
