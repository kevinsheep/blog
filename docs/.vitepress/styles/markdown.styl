  font-weight: 400;
  background-color: transparent;
}

/**
 * Code
     * -------------------------------------------------------------------------- */
/* inline code */
.md-preview :not(pre, h1, h2, h3, h4, h5, h6) > code {
  font-size: var(--vp-code-font-size);
}

.md-preview :not(pre) > code {
  border-radius: 4px;
  padding: 3px 6px;
  color: var(--vp-c-text-code);
  background-color: var(--vp-c-bg-mute);
  transition: color 0.5s, background-color 0.5s;
}

.md-preview h1 > code, .md-preview h2 > code, .md-preview h3 > code {
  font-size: 0.9em;
}

.md-preview a > code {
  color: var(--vp-c-brand);
  transition: color 0.25s;
}

.md-preview a:hover > code {
  color: var(--vp-c-brand-dark);
}

.md-preview div[class*='language-'] {
  position: relative;
  margin: 16px -24px;
  background-color: var(--vp-code-block-bg);
  overflow-x: auto;
  transition: background-color 0.5s;
}

@media (min-width: 640px) {
  .md-preview div[class*='language-'] {
    border-radius: 8px;
    margin: 16px 0;
  }
}

@media (max-width: 639px) {
  .md-preview li div[class*='language-'] {
    border-radius: 8px 0 0 8px;
  }
}

.md-preview div[class*='language-'] + div[class*='language-'], .md-preview div[class$='-api'] + div[class*='language-'], .md-preview div[class*='language-'] + div[class$='-api'] > div[class*='language-'] {
